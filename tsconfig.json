{
  "extends": "./tsconfig.projects.json",
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "types": ["vitest/importMeta", "vitest/globals", "@types/mdx"],
    "baseUrl": ".",
    "paths": {
      "@ui/*": ["./src/components/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "_data/i18n/*.json",
    "_data/i18n/intl/*.json",
    "./next-env.d.ts",
    "./src/**/*",
    "./.next/types/**/*.ts"
  ],
  "exclude": [
    "_data/categories",
    "_data/_dynamic",
    "_data/events",
    "_data/jobs",
    "_data/pages",
    "_data/posts",
    "_data/settings",
    "_data/topics",
    "_data/tutorials",
    ".yarn",
    "node_modules",
    "dist",
    "./workspaces/content-onedge",
    "./workspaces/netlify-cms",
    "./workspaces/static-cms"
  ],
  "references": [
    {
      "path": "./workspaces/cms-config"
    }
    // {
    //   "path": "./workspaces/content-onedge"
    // },
    // {
    //   "path": "./workspaces/netlify-cms"
    // },
    // {
    //   "path": "./workspaces/static-cms"
    // },
    // {
    //   "path": "./tsconfig.nextjs.json"
    // }
  ]
}
